{
  "name": "qcode-chat",
  "displayName": "QCode Chat",
  "description": "AI coding assistant with OpenRouter integration",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "media/icon.svg",
  "activationEvents": [
    "onCommand:qcode-chat.openChat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "qcode-chat.openChat",
        "title": "Open QCode Chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/node": "16.x",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
